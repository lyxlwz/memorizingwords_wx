.hasMargin .go-input-view {
  width: 100%;
  margin: 20rpx 20rpx;
  box-sizing: border-box;
  position: relative;
}
.onlyInput {
  position: relative;
}
.onlyInput .go-input-ctl {
  width: 100%;
  padding: 0 30rpx;
  box-sizing: border-box;
  position: relative;
  height: var(--go-item-height);
  background-color: var(--background-sub-color);
}
.go-input-view > text {
  display: block;
  width: 15%;
  font-size: calc(28rpx + var(--font-size-param));
  color: var(--input-label-color);
  margin-right: 21.7391rpx;
}
.go-input-view > view {
  width: 72%;
}
.go-input-view > view > view {
  margin-top: 2.1739 upx;
  margin-left: 0;
}
.go-input-ctl {
  border: none;
  /*我修改的*/
  border-bottom: 1px solid var(--border-color);
  width: 100%;
  font-size: calc(28rpx + var(--font-size-param));
  min-height: var(--go-item-height);
  transition: all .3s;
  line-height: var(--go-item-height);
  color: var(--go-input-color);
  box-sizing: border-box;
  padding: 0 10px 0 22rpx;
  position: relative;
  background: transparent;
}
.go-input-text-ctl {
  color: var(--go-input-color);
  background: transparent;
}
.go-input-ctl:hover,
.go-input-ctl:focus {
  /*原来的*/
  /*border: 1px solid #747474;*/
  /*我改的*/
  border-bottom: 1px solid #DFDFDF;
}
.go-txt-r {
  text-align: right;
}
.go-txt-l {
  text-align: left;
}
.go-txt-c {
  text-align: center;
}
.go-txt-top {
  width: 100% !important;
  margin-bottom: 6.5217rpx;
}
.go-error {
  border-bottom: 1px solid #DFDFDF !important;
}
.go-success {
  border-bottom: 2.1739rpx solid #008000 !important;
}
.go-input-textarea .go-error-tip.rowError {
  top: auto;
  bottom: 10rpx;
}
.go-error-tip {
  color: #de0000;
  position: absolute;
}
.clear-value-btn {
  position: absolute;
  top: 30%;
  right: 34rpx;
  cursor: pointer;
}
.show-pwd-icon,
.close-pwd-icon {
  position: absolute;
  top: 15rpx;
  right: 80rpx;
  cursor: pointer;
}
.go-error-tip.rowError {
  position: absolute;
  top: 30rpx;
  right: 32rpx;
}
@-webkit-keyframes left {
  0% {
    background-color: #c0c0c0;
    width: 0;
  }
  100% {
    background-color: #747474;
    width: 100%;
  }
}
@keyframes left {
  0% {
    background-color: #c0c0c0;
    width: 0;
  }
  100% {
    background-color: #747474;
    width: 100%;
  }
}
@-webkit-keyframes center {
  0% {
    background-color: #c0c0c0;
    width: 0;
    left: 50%;
  }
  100% {
    background-color: #747474;
    width: 100%;
    left: 0%;
  }
}
@keyframes center {
  0% {
    background-color: #c0c0c0;
    width: 0;
    left: 50%;
  }
  100% {
    background-color: #747474;
    width: 100%;
    left: 0%;
  }
}
:focus {
  outline: none;
}
.go-disabled {
  color: #c1c1c1 !important;
}
.u-tag--mini {
  padding: 0 1px !important;
}
.u-tag__text--mini {
  margin: 6rpx;
}
.u-form-item__body__left__content__required {
  top: 0 !important;
  font-size: calc(30rpx + var(--font-size-param)) !important;
}
.u-form-item__body__left__content__label {
  padding-left: 4rpx !important;
}
.u-button--plain.u-button--warning {
  color: #f9ae3d !important;
}
.u-cell {
  background-color: var(--background-sub-color);
}
